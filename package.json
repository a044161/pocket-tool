{
	"name": "pocket-tool",
	"version": "1.0.4",
	"description": "build own tools",
	"main": "index.js",
	"scripts": {
		"dev": "npm run clear && rollup -w --config rollup.dev.config.js",
		"build": "npm run clear && npm run test && npm run dist",
		"dist": "rollup --config rollup.config.js",
		"test-all": "npm run test && npm run test-browser",
		"test": "npm run dist && mocha",
		"test-browser":
			"phantomjs ./node_modules/mocha-phantomjs-core/mocha-phantomjs-core.js ./test/test-browser/index.html spec '{\"useColors\":true}'",
		"clear": "rm -rf dist/"
	},
	"repository": {
		"type": "git",
		"url": "git+git@github.com:a044161/pocket-tool.git"
	},
	"keywords": ["tools"],
	"author": "weng wang",
	"license": "ISC",
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-plugin-external-helpers": "^6.22.0",
		"babel-preset-latest": "^6.24.1",
		"mocha-phantomjs-core": "^2.1.2",
		"rollup-plugin-babel": "^3.0.2",
		"rollup-plugin-node-resolve": "^3.0.0",
		"rollup-plugin-uglify": "^2.0.1",
		"should": "^13.0.1",
		"uglify-es": "^3.1.3"
	}
}
